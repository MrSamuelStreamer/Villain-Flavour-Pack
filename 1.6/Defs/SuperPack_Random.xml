<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <SoundDef>
        <defName>RMP_NumberDestroy</defName>
        <context>MapOnly</context>
        <maxSimultaneous>1</maxSimultaneous>
        <subSounds>
            <li>
                <grains>
                    <li Class="AudioGrain_Clip">
                        <clipPath>RMP_NumberDestroy</clipPath>
                    </li>
                </grains>
                <pitchRange>0.8~1.2</pitchRange>
                <volumeRange>30~50</volumeRange>
            </li>
        </subSounds>
    </SoundDef>

    <ThingDef ParentName="BuildingBase" Name="SP_Number">
        <rotatable>false</rotatable>
        <size>(3,3)</size>
        <fillPercent>1</fillPercent>
        <altitudeLayer>Building</altitudeLayer>
        <statBases>
            <MaxHitPoints>50</MaxHitPoints>
            <Flammability>0</Flammability>
        </statBases>
        <passability>PassThroughOnly</passability>
        <pathCost>50</pathCost>
        <tickerType>Normal</tickerType>
        <building>
            <claimable>false</claimable>
            <alwaysDeconstructible>true</alwaysDeconstructible>
            <canLandGravshipOn>true</canLandGravshipOn>
            <isInert>true</isInert>
        </building>
        <comps>
            <li Class="SuperPack.CompProperties_DestroyAfter">
                <ticks>600~1200</ticks>
                <explodeOnDestroy>true</explodeOnDestroy>
            </li>
        </comps>
    </ThingDef>

